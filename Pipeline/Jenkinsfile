pipeline {
    agent any
    stages {
        stage('Print WorkSpace') {
            steps {
                script {
				    echo "Hello world"
                    echo "当前的工作目录: ${env.WORKSPACE}"
                }
            }
        stage('执行python脚本'){
            steps {
                sh "python ${env.WORKSPACE}/tt.py"
            }
        }
        }
    }
}